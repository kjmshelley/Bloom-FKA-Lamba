"components/App.js","original":{"line":69,"column":16}},{"name":"quotes","generated":{"line":93,"column":12},"source":"components/App.js","original":{"line":69,"column":18}},{"generated":{"line":93,"column":18},"source":"components/App.js","original":{"line":68,"column":8}},{"generated":{"line":94,"column":0},"source":"components/App.js","original":{"line":70,"column":10}},{"generated":{"line":94,"column":4},"source":"components/App.js","original":{"line":70,"column":10}},{"generated":{"line":94,"column":12},"source":"components/App.js","original":{"line":70,"column":18}},{"name":"deleteQuote","generated":{"line":94,"column":14},"source":"components/App.js","original":{"line":70,"column":20}},{"generated":{"line":95,"column":0},"source":"components/App.js","original":{"line":68,"column":8}},{"generated":{"line":95,"column":4},"source":"components/App.js","original":{"line":61,"column":6}},{"generated":{"line":95,"column":5},"source":"components/App.js","original":{"line":60,"column":4}},{"generated":{"line":95,"column":6},"source":"components/App.js","original":{"line":59,"column":2}},{"generated":{"line":96,"column":0},"source":"components/App.js","original":{"line":74,"column":1}}],"sources":{"components/App.js":"import React, { useEffect } from 'react';\nimport { Link } from \"react-router-dom\";\nimport { useSelector, useDispatch } from \"react-redux\";\n\nimport { get, del } from '../api';\nimport QuoteList from \"./Quotes\";\n\nexport default function App() {\n  const quotes = useSelector(state => state.quotes);\n  const dispatch = useDispatch();\n\n  const getQuotes = () => {\n    (async () => {\n      try {\n        /*************** TASK 1*************\n         * Use 'axios' or 'fetch' to [GET] a list of quotes\n         * from 'http://localhost:3333/api/quotes'. On success, the quotes\n         * in the response body should be set as the 'quotes' slice of state.\n         * On error, 'handleError' should be called.`\n         * ***********************************************/\n        const data = await get('/quotes');\n        dispatch({ type: \"app/load\", payload: data });\n      } catch (ex) {\n        handleError(ex);\n      }\n    })();\n  }\n\n\n  const deleteQuote = async (id) => {\n    /***************** TASK 4 *******************\n     * - Use 'axios' or 'fetch' to [DELETE] an existing quote\n     * in 'http://localhost:3333/api/quotes/:id'. On success, the deleted quote\n     * should be removed from the 'quotes' slice of state'.\n     * On error, 'handleError' should be called. Finally, the form should be reset.\n      ******************************************* */\n    if(!id) return;\n\n    /* I just used an alert for simplicity */\n    let shouldDelete = confirm(\"Are you sure you want to delete this quote?\");\n    if (!shouldDelete) return;\n\n    try {\n        const data = await del(`/quotes/${id}`);\n        if (data !== id) throw Error(\"Delete unsuccessful\");\n\n        dispatch({ type: \"quote/delete\", payload: quotes.filter(q => q.id !== id) });\n\n    } catch (ex) {\n        console.log(ex);\n        handleError();\n    } \n  }\n\n  const handleError = err => { debugger } // eslint-disable-line\n\n  useEffect(() => getQuotes(), [])\n\n  return (\n    <div className=\"container\">\n      <div className=\"card\">\n        <div className=\"header\">\n          <div className=\"title\">Quotes of the Day</div>\n          <div className=\"button add\">\n            <Link to={`/quotes/add`}>Add</Link>\n          </div>\n        </div>\n        <QuoteList\n          quotes={quotes}\n          onDelete={deleteQuote} />\n      </div>\n    </div>\n  )\n}\n"},"lineCount":null}},"error":null,"hash":"e3558e1020983bccb6bd954e299a7632","cacheData":{"env":{}}}